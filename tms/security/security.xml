<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="group_model" model="res.groups">
            <field name="name">Group Model</field>
        </record>
        <record id="group_monitoring" model="res.groups">
            <field name="name">Group Monitoring</field>
            <field name="implied_ids" eval="[(4, ref('base_geoengine.group_geoengine_user'))]"/>
        </record>
        <record id="group_traffic" model="res.groups">
            <field name="name">Group Traffic</field>
            <field name="implied_ids" eval="[(4, ref('account.group_account_invoice')), (4, ref('base_geoengine.group_geoengine_user'))]"/>
        </record>
        <record id="group_supervisor_traffic" model="res.groups">
            <field name="name">Group Supervisor Traffic</field>
            <field name="implied_ids" eval="[(4, ref('base_geoengine.group_geoengine_user')), (4, ref('account.group_account_invoice')), (4, ref('group_traffic'))]"/>
        </record>
        <record id="group_expenses" model="res.groups">
            <field name="name">Group Expenses</field>
             <field name="implied_ids" eval="[(4, ref('group_monitoring')), (4, ref('account.group_account_invoice')), (4, ref('base_geoengine.group_geoengine_user'))]"/>
        </record>
        <record id="group_treasury" model="res.groups">
            <field name="name">Group Treasury</field> 
        </record>
    </data>
</odoo>
