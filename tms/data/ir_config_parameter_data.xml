        <!-- Vehicle update distance parameter -->
        <record id="tms_property_update_vehicle_distance" model="ir.config_parameter">
            <field name="key">tms_property_update_vehicle_distance</field>
            <field name="value">2
  1- Update distance from Travels
  2- Update distance from Travel Expense Record
  3- Update Manually
  4- Update from Tyre Revisions
      </field>
        </record>
